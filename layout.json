{
 "info": {"version": 1, "license": "GPL-2.1-or-later", "author": "Cyril Hrubis <metan@ucw.cz>"},
 "layout": {
  "rows": 2,
  "align": "fill",
  "rfill": "0, 1",
  "border": "none",
  "pad": 0,
  "widgets": [
   {
    "cols": 12,
    "halign": "left",
    "widgets": [
     {
      "type": "button",
      "btype": "first",
      "on_event": "button_first_event"
     },
     {
      "type": "button",
      "btype": "prev",
      "on_event": "button_prev_event"
     },
     {
      "type": "button",
      "btype": "next",
      "on_event": "button_next_event"
     },
     {
      "type": "button",
      "btype": "last",
      "on_event": "button_last_event"
     },
     {
      "type": "label",
      "text": "Page"
     },
     {
      "type": "tbox",
      "text": "1",
      "len": 4,
      "uid": "pg_nr",
      "on_event": "load_page_event"
     },
     {
      "type": "label",
      "text": "of ????",
      "uid": "pg_cnt"
     },
     {
      "type": "tbox",
      "len": 10,
      "on_event": "tbox_search_event"
     },
     {
      "type": "spinbutton",
      "choices": ["fit width", "fit page", "125%", "150%"]
     },
     {
      "type": "button",
      "btype": "rotate_cw",
      "on_event": "rotate_cw"
     },
     {
      "type": "button",
      "btype": "rotate_ccw",
      "on_event": "rotate_ccw"
     },
     {
      "type": "button",
      "label": "Open",
      "btype": "open",
      "on_event": "button_open_file"
     }
    ]
   },
   {
    "align": "fill",
    "type": "pixmap",
    "w": 200,
    "h": 200,
    "uid": "page"
   }
  ]
 }
}
